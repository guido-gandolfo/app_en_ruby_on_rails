<h2>Registrarme</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :nombre %><br />
    <%= f.text_field :name, autocomplete: "Ingrese su nombre" %>
  </div>

  <div class="field">
    <%= f.label :apellido %><br />
    <%= f.text_field :apellido, autocomplete: "Ingrese su apellido" %>
  </div>

  <div class="field">
    <%= f.label :DNI %><br />
    <%= f.text_field :DNI, autocomplete: "Ingrese su DNI" %>
  </div>

  <div class="field">
    <%= f.label :nacimiento %><br />
    <%= f.date_field :nacimiento, autocomplete: "Ingrese su nacimiento" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :contrase単a %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "nueva contase単a" %>
  </div>

  <div class="field">
    <%= f.label :confirmar_contrase単a %><br />
    <%= f.password_field :password_confirmation, autocomplete: "nueva contase単a" %>
  </div>

  <div class="actions">
    <%= f.submit "Registrarme" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
