<%= form_with(model: vaccination) do |centro| %>
  <% if vaccination.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(vaccination.errors.count, "error") %> prohibited this vaccination from being saved:</h2>

      <ul>
        <% vaccination.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class='field'>
  <div class="contenedor-ficha"
      <div class="contenedor-campo, bloque">
          <p class="tamanio-fuente">
          
            <strong><%=centro.label :"zona:"%></strong>
            <%= @vaccination.zona%>
          </p> 
      </div>
      <%=centro.hidden_field :zona, class:"form-ficha"%>
  </div>
  <div class="campo-descripcion">
      <div class='field'>
          <strong><%=centro.label :"direccion:", class:"tamanio-fuente"%></strong>
          <%=centro.text_field :direccion, class:"form-ficha"%>
      </div>
      <div class="actions">
          <%= centro.submit "Modificar" %>
      </div>
  </div>
<% end %>
